<body>
    <section class="light-content services">
        <div class="container">
            <div class="row">

                <div class="col-md-4 col-sm-4">
                    <div class="service-box-wrap">
                        <div class="service-icon-wrap">
                            <i class="fa fa-black-tie fa-2x"></i>
                        </div> <!-- /.service-icon-wrap -->
                        <div class="service-cnt-wrap">
                            <h3 class="service-title">Current Customers</h3>
                            <p>@ViewData["customers"]  </p>
                            @Html.ActionLink("Manage", "Index", "Customer", null, new { @class = "btn btn-primary" })

                        </div> <!-- /.service-cnt-wrap -->
                    </div> <!-- /.service-box-wrap -->
                </div> <!-- /.col-md-4 -->

                <div class="col-md-4 col-sm-4">
                    <div class="service-box-wrap">
                        <div class="service-icon-wrap">
                            <i class="fa fa-camera-retro fa-2x"></i>
                        </div> <!-- /.service-icon-wrap -->
                        <div class="service-cnt-wrap">
                            <h3 class="service-title">Current Movies</h3>
                            <p>@ViewData["movies"]  </p>
                            @Html.ActionLink("Manage", "moviesView", "Movie", null, new { @class = "btn btn-primary" })
                        </div> <!-- /.service-cnt-wrap -->
                    </div> <!-- /.service-box-wrap -->
                </div> <!-- /.col-md-4 -->

                <div class="col-md-4 col-sm-4">
                    <div class="service-box-wrap">
                        <div class="service-icon-wrap">
                            <i class="fa fa-pencil-square-o fa-2x"></i>
                        </div> <!-- /.service-icon-wrap -->
                        <div class="service-cnt-wrap">
                            <h3 class="service-title">Current Rentals</h3>
                            <p>@ViewData["rentals"]  </p>
                            @Html.ActionLink("Manage", "moviesView", "Movie", null, new { @class = "btn btn-primary" })
                        </div> <!-- /.service-cnt-wrap -->
                    </div> <!-- /.service-box-wrap -->
                </div> <!-- /.col-md-4 -->

            </div>

    </section>
